{
  "name": "Online Louvre",
  "version": "1.0.0",
  "description": "This package.json holds the dependencies of all the backend services of lirtenhub-notification",
  "main": "server.js",
  "dependencies": {
    "axios": "^0.19.0",
    "bcrypt": "^3.0.6",
